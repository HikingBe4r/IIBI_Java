import java.util.*;

public class DataType7 {
	public static void main(String[] args) {
		int a, b;
		Scanner s = new Scanner(System.in);
		a = s.nextInt();
		b = s.nextInt();
		
		System.out.printf("%d\n", a+b);
		System.out.printf("%d\n", a-b);
		System.out.printf("%d\n", a*b);
		System.out.printf("%d\n", a/b);
		System.out.printf("%d\n", a%b);
	}
}