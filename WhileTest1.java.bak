//import, Package

public class WhileTest1{
	// Global variable

	// main method
	public static void main(String[] args) {
		
		// Local variable
		int i = 1;

		while (i <= 5) {
			System.out.printf("%d%n", i);
			
			i++;
		}

		int dw1 = 0;
		do {
			System.out.printf("dw1: %d%n", dw1);
			dw1++;
		}
		while (dw1 == 3);


	}
}
